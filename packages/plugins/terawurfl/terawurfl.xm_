<?xml version="1.0" encoding="utf-8"?>
<install version="1.5" type="plugin" group="mobile" method="upgrade">
	<name>Mobile - TeraWURFL</name>
	<version>###VERSION###</version>
	<author>###AUTHOR###</author>
	<creationDate>###DATE###</creationDate>
	<copyright>###COPYRIGHT###</copyright>
	<license>###LICENSE###</license>
	<authorEmail>###EMAIL###</authorEmail>
	<authorUrl>###URL###</authorUrl>
	<description>PLG_MOBILE_TERAWURFL__TERAWURFL_CHECKER</description>
	<files>
		<filename plugin="terawurfl">terawurfl.php</filename>
		<folder>terawurfl</folder>
	</files>
	<languages>
		<language tag="en-GB">en-GB.plg_mobile_terawurfl.ini</language>
	</languages>
	<params>
		<param name="mysql4" type="radio" default="0" label="PLG_MOBILE_TERAWURFL__MYSQL4_LABEL" description="PLG_MOBILE_TERAWURFL__MYSQL4_DESC">
			<option value="0">PLG_MOBILE_TERAWURFL__USEMYSQL5</option>
			<option value="1">PLG_MOBILE_TERAWURFL__MYSQL4</option>
		</param>
		<param name="cache" type="radio" default="0" label="PLG_MOBILE_TERAWURFL__CACHE_LABEL" description="PLG_MOBILE_TERAWURFL__CACHE_DESC">
			<option value="0">PLG_MOBILE_TERAWURFL__NO</option>
			<option value="1">PLG_MOBILE_TERAWURFL__YES</option>
		</param>
	</params>
	<config><fields name="params">
	<fieldset name="basic">
		<field name="mysql4" type="radio" default="0" label="PLG_MOBILE_TERAWURFL__MYSQL4_LABEL" description="PLG_MOBILE_TERAWURFL__MYSQL4_DESC">
			<option value="0">PLG_MOBILE_TERAWURFL__USEMYSQL5</option>
			<option value="1">PLG_MOBILE_TERAWURFL__MYSQL4</option>
		</field>
		<field name="cache" type="radio" default="0" label="PLG_MOBILE_TERAWURFL__CACHE_LABEL" description="PLG_MOBILE_TERAWURFL__CACHE_DESC">
			<option value="0">PLG_MOBILE_TERAWURFL__NO</option>
			<option value="1">PLG_MOBILE_TERAWURFL__YES</option>
		</field>
	</fieldset>
	</fields></config>
</install>